# AI API Keys - Multiple keys supported for each provider
# Format: PROVIDER_API_KEY, PROVIDER_API_KEY_1, PROVIDER_API_KEY_2, etc.

# Gemini API Keys (Primary)
GEMINI_API_KEY=your_gemini_key_1
GEMINI_API_KEY_1=your_gemini_key_2
GEMINI_API_KEY_2=your_gemini_key_3

# Groq API Keys (Fast inference)
GROQ_API_KEY=your_groq_key_1
GROQ_API_KEY_1=your_groq_key_2
GROQ_API_KEY_2=your_groq_key_3

# OpenAI API Keys
OPENAI_API_KEY=your_openai_key_1
OPENAI_API_KEY_1=your_openai_key_2

# OpenRouter API Keys
OPENROUTER_API_KEY=your_openrouter_key_1
OPENROUTER_API_KEY_1=your_openrouter_key_2

# GitHub Models API Keys
GITHUB_API_KEY=your_github_key_1
GITHUB_API_KEY_1=your_github_key_2

# Deapi Keys (Flux AI)
DEAPI_API_KEY=your_deapi_key_1
DEAPI_API_KEY_1=your_deapi_key_2

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Example of how to add more keys:
# GEMINI_API_KEY_3=another_gemini_key
# GEMINI_API_KEY_4=yet_another_gemini_key
# GROQ_API_KEY_3=another_groq_key